<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Products</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <h1>Product List</h1>
  <nav>
    <a href="/">Home</a>
    <a href="/user">Users</a>
    <a href="/products/filtered">Filtered Products</a>
  </nav>
  <table>
    <tr>
      <th>Product ID</th>
      <th>Name</th>
      <th>Price</th>
    </tr>
    <% products.forEach(product => { %>
    <tr>
      <td><%= product.PID %></td>
      <td><%= product.name %></td>
      <td><%= product.price %></td>
    </tr>
    <% }) %>
  </table>
</body>
</html>
